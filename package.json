{
  "name": "aws-cognito-cli",
  "version": "1.0.6",
  "type": "module",
  "description": "A cli to interact with your cognito user pool, create accounts, force verify, login, verify phone number, verify email, etc.",
  "repository": {
    "type": "git",
    "url": "git://github.com/wednesday-solutions/cognito-cli.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=12"
  },
  "author": "Wednesday Solutions",
  "license": "MIT",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.js",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "#npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "initialize": "git checkout --orphan temp-branch && git add -A && git commit -m 'Initial commit' && git branch -D master && git branch -m master"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "keywords": [
    "aws",
    "cognito",
    "cli"
  ],
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "aws-amplify": "^3.3.17",
    "aws-sdk": "^2.836.0",
    "babel-eslint": "10.0.1",
    "circular-dependency-plugin": "5.0.2",
    "coveralls": "3.0.3",
    "fs": "^0.0.1-security",
    "json": "^9.0.6",
    "lint-staged": "8.1.5",
    "minimist": "1.2.3",
    "node-fetch": "^2.6.1",
    "node-plop": "0.18.0",
    "plop": "^2.5.2",
    "pre-commit": "1.2.2",
    "prettier": "1.17.0",
    "prettier-config-standard": "^1.0.1",
    "readline": "^1.3.0",
    "shelljs": "^0.8.3",
    "url": "^0.11.0"
  },
  "@babel/cli": "7.4.3",
  "bin": {
    "cognito-cli": "bin/cognito-cli.mjs"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
